<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomButton">
        <attr name="button_text" format="string"/>
        <attr name="button_text_color" format="integer|reference"/>
        <attr name="loading_color" format="integer|reference"/>
        <attr name="has_icon" format="boolean"/>
        <attr name="button_icon" format="integer|reference"/>
        <attr name="button_icon_color" format="integer|reference"/>
        <attr name="button_background" format="integer|reference"/>
        <attr name="success_button_background" format="integer|reference"/>
        <attr name="success_icon_color" format="integer|reference"/>
        <attr name="match_parent" format="boolean"/>
    </declare-styleable>
</resources>